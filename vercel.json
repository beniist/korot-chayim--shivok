{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/interactive",
      "destination": "/resume-interactive.html"
    },
    {
      "source": "/classic",
      "destination": "/resume.html"
    },
    {
      "source": "/print",
      "destination": "/resume-print.html"
    },
    {
      "source": "/template",
      "destination": "/resume-template.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "silent": true
  }
} 